{
  "name": "deploy",
  "title": "eLife",
  "version": "0.0.1",
  "private": true,
  "description": "eLife Deploy",
  "homepage": "https://github.com/elifesciences/elife-dashboard",
  "author": {
    "name": "eLife",
    "url": "http://elifesciences.org/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elifesciences/elife-dashboard.git"
  },
  "scripts": {
    "install": "grunt"
  },
  "devDependencies": {
    "browserify": "~13.0.1",
    "browserify-shim": "~3.8.12",
    "browserify-test": "~2.1.2",
    "chai": "~3.5.0",
    "coffee-script": "^1.12.7",
    "grunt": "~1.0.4",
    "grunt-browserify": "^5.3.0",
    "grunt-cli": "~1.2.0",
    "grunt-contrib-concat": "~1.0.1",
    "grunt-contrib-handlebars": "~1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-sass": "~2.0.0",
    "grunt-scss-lint": "^0.5.0",
    "grunt-shell": "~1.3.1",
    "jsdom": "~9.2.1",
    "loglevel": "~1.4.1",
    "mocha": "~2.5.3",
    "mocha-headless-chrome": "^3.1.0",
    "sinon": "~1.17.4"
  },
  "dependencies": {
    "bootstrap-sass": "~3.4.1",
    "font-awesome": "~4.6.3",
    "fontawesome": "~4.5.0",
    "fullcalendar": "^2.6.1",
    "handlebars": "^4.7.6",
    "hbsfy": "~2.7.0",
    "historyjs": "~1.8.0-b2",
    "jquery": "~3.5.0",
    "moment": "~2.19.3",
    "pickadate": "~3.5.6",
    "qtip2": "~3.0.3",
    "underscore": "~1.8.3",
    "uniq": "~1.0.1"
  },
  "browserify": {
    "transform": [
      [
        "hbsfy",
        "browserify-shim"
      ]
    ]
  },
  "browser": {
    "pickadate": "./assets/libs/pickadate/lib/index.js",
    "swag": "./assets/libs/swag.js",
    "templates": "./assets/js/templates.js",
    "utils": "./assets/js/helpers/utils.js",
    "validate": "./assets/js/helpers/validate.js",
    "detail": "./assets/js/pages/detail.js",
    "history": "./assets/libs/jquery.history.js"
  },
  "browserify-shim": {
    "swag": "Swag",
    "jquery": "$",
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    },
    "fullcalendar": {
      "depends": [
        "jquery:$",
        "moment"
      ]
    },
    "history": {
      "exports": "history",
      "depends": [
        "jquery:$"
      ]
    }
  }
}
